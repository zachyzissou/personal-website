<?xml version="1.0"?>
<Container version="2">
  <Name>personal-website</Name>
  <Repository>ghcr.io/zachyzissou/personal-website:latest</Repository>
  <Registry>https://github.com/zachyzissou/personal-website/pkgs/container/personal-website</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/zachyzissou/personal-website</Support>
  <Project>https://zachgonser.com</Project>
  <Overview>Modern personal portfolio website built with Astro, React, and Tailwind CSS. Features responsive design, animations, and container showcase.</Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:18475]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/zachyzissou/personal-website/main/personal-website.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/zachyzissou/personal-website/main/public/favicon.svg</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Web Port" Target="8080" Default="18475" Mode="tcp" Description="Port for accessing the website" Type="Port" Display="always" Required="true" Mask="false">18475</Config>
</Container>